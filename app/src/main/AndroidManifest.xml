<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="azmimuhammad.com.materialdesign">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppMaterialTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".widget.ToolbarActivity" />
        <activity android:name=".widget.StandAloneActivity" />
        <activity android:name=".widget.ActionBarActivity" />
        <activity android:name=".widget.ContextualMenuActivity" />
        <activity android:name=".widget.SnackBarActivity" />
        <activity android:name=".widget.FabActivity" />
        <activity android:name=".feedback.AnimationActivity" />
        <activity android:name=".feedback.RippleActivity" />
        <activity android:name=".feedback.SharedElementActivity" />
        <activity android:name=".feedback.TransitionActivity" />
        <activity android:name=".login.LoginActivity" />
        <activity android:name=".appbarlayout.AppBarActivity" />
        <activity android:name=".appbarlayout.AppBarImageActivity" />
        <activity android:name=".appbarlayout.MainAppBarActivity" />
        <activity android:name=".navigationview.NavigationViewActivity" />
        <activity android:name=".navigationview.SimpleNavigationViewActivity" />
        <activity android:name=".navigationview.GroupNavigationViewActivity" />
        <activity android:name=".recyclerview.RecyclerViewActivity"></activity>
    </application>

</manifest>